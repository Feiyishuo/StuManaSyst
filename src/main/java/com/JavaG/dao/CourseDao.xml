<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.JavaG.dao.CourseDao">
    <select id="findAllCourseByStudentId" parameterType="java.lang.Integer" resultType="com.JavaG.domain.Course">
        SELECT *
        FROM course
        WHERE id IN (SELECT cId as id FROM sc WHERE sId like #{id})
    </select>
</mapper>